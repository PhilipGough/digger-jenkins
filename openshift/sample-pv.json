{
      "kind": "PersistentVolume",
      "apiVersion": "v1",
      "metadata": {
        "name": "jenkins-dev-pv1"
      },
      "spec": {
        "capacity": {
          "storage": "50Gi"
        },
        "accessModes": [
          "ReadWriteOnce"
        ],
        "persistentVolumeReclaimPolicy": "Recycle",
        "hostPath": {
          "path": "/tmp/jenkins"
        }
      }
}